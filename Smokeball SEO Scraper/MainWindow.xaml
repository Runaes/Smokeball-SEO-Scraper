<Window x:Class="Smokeball_SEO_Scraper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Smokeball_SEO_Scraper"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Settings" Content="Settings" HorizontalAlignment="Left" Margin="245,330,0,0" VerticalAlignment="Top" Width="89" RenderTransformOrigin="0.498,0.644" Click="Settings_Click"/>
        <Button x:Name="Activator" Content="Run" HorizontalAlignment="Left" Margin="85,330,0,0" VerticalAlignment="Top" Width="92" Click="Activator_Click"/>
        <TextBlock x:Name="Output" HorizontalAlignment="Left" Margin="101,189,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="127" Width="218"/>
        <WebBrowser x:Name="Browser" Margin="391,10,10,10"/>
    </Grid>
</Window>
